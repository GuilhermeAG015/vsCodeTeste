        "*.template": "html",
        "*.html": "html"
    },
    "files.autoSave": "off",
    "workbench.colorTheme": "NarutoDark",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "workbench.iconTheme": "material-icon-theme",
